{
  "name": "Spotify friends",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Extension for enhanced Spotify listening with friends",
  "icons": {
    "128": "assets/icons/icon-128x128.png"
  },
  "permissions": [],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "assets/icons/icon-128x128.png",
    "default_popup": "popup.html"
  },
  "options_page": "index.html"
}
